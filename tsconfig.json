{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "files": [],
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "typeRoots": ["./node_modules/@types", "./types", "src/types"],
    "esModuleInterop": true,
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "importHelpers": true,
    "resolveJsonModule": true,
    "lib": ["esnext", "dom"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "types/**/*.d.ts",
    ".nuxt/nuxt.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/**/*.vue",
    "components/**/*.vue",
    "pages/**/*.vue",
    "components/canvases/class/Confetti.ts",
    "_old/Confetti001.vue",
    "_old/Layer_Labyrinth001 copy.vue"
  ],
  "exclude": ["src/**/__tests__/*"]
}
